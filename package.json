{
  "name": "@core/data",
  "version": "0.0.14",
  "description": "GitPod for Typescript",
  "repository": "https://github.com/chlbri/core-data.git",
  "author": "chlbri <bri_lvi@icloud.com>",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm test && pnpm build:-test",
    "build:-test": "pnpm tsc && pnpm lint",
    "clear:test": "jest --clearCache",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx,yml,json}\"",
    "lint": "pnpm prettier && pnpm eslint -- --fix",
    "rm": "rimraf -rf node_modules pnpm-lock.yaml",
    "rinit": "pnpm rm && pnpm pnpm",
    "publish": "pnpm run rm && pnpm run clean && pnpm run pnpm",
    "pnpm": "pnpm install",
    "upgrade": "pnpm upgrade",
    "upgrade-internal": "pnpm upgrade core core-promises core-test",
    "git-push": "git push --follow-tags",
    "git-build-push": "pnpm pnpm && pnpm build && pnpm git-push",
    "git-publish": "pnpm git-build-push && git checkout main && git merge dev && pnpm git-push && git checkout dev"
  },
  "devDependencies": {
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.12",
    "@types/object.omit": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "dayjs": "^1.11.11",
    "dequal": "^2.0.3",
    "edit-json-file": "^1.8.0",
    "eslint": "^9.2.0",
    "faker": "^6.6.6",
    "immer": "^10.1.1",
    "jest": "^29.7.0",
    "jest-cucumber": "^4.2.0",
    "jest-extended": "^4.0.2",
    "mock-stdin": "^1.0.0",
    "mongodb": "^6.6.0",
    "nanoid": "^5.0.7",
    "object.omit": "^3.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "zod": "^3.23.6",
    "zod-extended": "^1.0.5"
  },
  "peerDependencies": {
    "core": "github:chlbri/core",
    "core-promises": "github:chlbri/core-promises",
    "zod": "^3.10.1"
  },
  "dependencies": {
    "@bemedev/return-data": "^0.2.0",
    "@bemedev/zod-extended": "^0.1.2",
    "hotscript": "^1.0.13"
  }
}
