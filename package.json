{
  "name": "gitpod-typescript",
  "version": "0.0.1",
  "description": "GitPod for Typescript",
  "repository": "https://github.com/chlbri/core-data.git",
  "author": "chlbri <bri_lvi@icloud.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "build": "yarn run tsc && yarn run lint",
    "test": "jest",
    "prepare": "tsc",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "yarn run prettier && yarn run eslint --fix",
    "rm": "./node_modules/.bin/rimraf -rf node_modules yarn.lock",
    "rinit": "yarn run rm && yarn cache clean && yarn run yarn",
    "publish": "yarn run rm && yarn run clean && yarn run yarn",
    "clean": "yarn cache clean",
    "yarn": "yarn install",
    "git-push": "git push --tags"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "dayjs": "^1.10.7",
    "edit-json-file": "^1.6.0",
    "eslint": "^8.0.1",
    "faker": "^5.5.3",
    "immer": "^9.0.6",
    "jest": "^27.3.0",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^1.1.0",
    "mongodb": "^4.1.3",
    "nanoid": "^3.1.30",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "core": "https://github.com/chlbri/core.git#publish"
  }
}
