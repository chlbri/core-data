{
  "name": "@core/data",
  "version": "0.0.14",
  "description": "GitPod for Typescript",
  "repository": "https://github.com/chlbri/core-data.git",
  "type": "module",
  "author": "chlbri <bri_lvi@icloud.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test && pnpm run build",
    "rollup": "rollup -c",
    "build": "rm -rf lib && pnpm run rollup",
    "rm": "rm -rf node_modules pnpm-lock.yaml",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rm && pnpm run config:off",
    "test": "pnpm run test:coverage --no-coverage",
    "test:typecheck": "pnpm run test:coverage --no-coverage --typecheck",
    "test:coverage": "vitest run --passWithNoTests",
    "test:coverage:typecheck": "pnpm run test:coverage --typecheck",
    "test:watch": "vitest --passWithNoTests",
    "remove": "rm -rf lib node_modules pnpm-lock.yaml",
    "clean": "pnpm run remove && pnpm run config",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "prepublishOnly": "",
    "prepare": "husky"
  },
  "devDependencies": {
    "@bemedev/vitest-alias": "^0.0.2",
    "@bemedev/vitest-extended": "^0.3.3",
    "@types/faker": "^6.6.9",
    "@types/object.omit": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^1.6.0",
    "dayjs": "^1.11.11",
    "dequal": "^2.0.3",
    "edit-json-file": "^1.8.0",
    "eslint": "^9.2.0",
    "faker": "^6.6.6",
    "immer": "^10.1.1",
    "mock-stdin": "^1.0.0",
    "mongodb": "^6.6.0",
    "nanoid": "^5.0.7",
    "object.omit": "^3.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "tsd": "^0.31.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "zod": "^3.23.6",
    "zod-extended": "^1.0.5"
  },
  "peerDependencies": {
    "core": "github:chlbri/core",
    "core-promises": "github:chlbri/core-promises",
    "zod": "^3.10.1"
  },
  "dependencies": {
    "@bemedev/decompose": "^0.2.1",
    "@bemedev/return-data": "^0.2.0",
    "@bemedev/zod-extended": "^0.1.2",
    "deep-merge": "^1.0.0",
    "hotscript": "^1.0.13",
    "ts-deepmerge": "^7.0.0"
  }
}
